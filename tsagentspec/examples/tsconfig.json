{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "paths": {
      "agentspec": ["../src/index.ts"]
    },
    "noEmit": true
  },
  "include": ["./*.ts"]
}
